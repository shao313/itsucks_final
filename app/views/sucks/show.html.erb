<div class="page-header">
  <h3>Suck #<%= @suck.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @suck.user_id %></dd>

      <dt>Complaint</dt>
      <dd><%= @suck.complaint_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/sucks" class="btn btn-primary">
        Back
      </a>
      <a href="/sucks/<%= @suck.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_suck/<%= @suck.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
